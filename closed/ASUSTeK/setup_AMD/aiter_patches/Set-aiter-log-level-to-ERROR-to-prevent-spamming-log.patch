From 71017ae938c1aff9eee344251bb3711b6dc6719d Mon Sep 17 00:00:00 2001
From: MLPerf <mlperf>
Date: Wed, 1 Jan 2025 00:00:00 +0000
Subject: [PATCH] Set aiter log level to ERROR to prevent spamming logs

---
 aiter/__init__.py | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/aiter/__init__.py b/aiter/__init__.py
index ef1db690..caea5b41 100644
--- a/aiter/__init__.py
+++ b/aiter/__init__.py
@@ -12,7 +12,7 @@ logger = logging.getLogger("aiter")
 def getLogger():
     global logger
     if not logger.handlers:
-        logger.setLevel(logging.DEBUG)
+        logger.setLevel(logging.ERROR)
 
         console_handler = logging.StreamHandler()
         if int(os.environ.get("AITER_LOG_MORE", 0)):
@@ -25,7 +25,7 @@ def getLogger():
                 fmt="[%(name)s] %(message)s",
             )
         console_handler.setFormatter(formatter)
-        console_handler.setLevel(logging.INFO)
+        console_handler.setLevel(logging.ERROR)
         logger.addHandler(console_handler)
 
     return logger
-- 
